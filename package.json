{
  "name": "deptox",
  "private": true,
  "version": "0.1.10",
  "description": "Reclaim disk space by finding and removing dependency directories",
  "author": "Alex Whinfield",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexwhin/deptox"
  },
  "bugs": {
    "url": "https://github.com/alexwhin/deptox/issues"
  },
  "homepage": "https://github.com/alexwhin/deptox",
  "type": "module",
  "scripts": {
    "dev": "tauri dev",
    "build": "tsc && vite build",
    "tauri": "tauri",
    "lint": "tsc --noEmit && eslint src/ && (cd src-tauri && cargo clippy -- -D warnings)",
    "lint:ts": "tsc --noEmit && eslint src/",
    "lint:eslint": "eslint src/",
    "lint:eslint:fix": "eslint src/ --fix",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "test": "vitest run && cd src-tauri && cargo test",
    "test:ts": "vitest run",
    "test:ts:watch": "vitest",
    "test:ts:coverage": "vitest run --coverage",
    "test:rust": "cd src-tauri && cargo test",
    "test:rust:coverage": "cd src-tauri && cargo tarpaulin --out Html --output-dir ../coverage-rust",
    "test:coverage": "vitest run --coverage && cd src-tauri && cargo tarpaulin --out Html --output-dir ../coverage-rust",
    "audit:deps": "pnpm audit --audit-level=high",
    "audit:rust": "cd src-tauri && cargo audit",
    "audit:knip": "knip",
    "audit:circular": "madge --circular src/",
    "audit:versions": "syncpack list-mismatches",
    "audit:secrets": "secretlint 'src/**' 'src-tauri/src/**' 'locales/**' '.github/**' '*.json' '*.md' '*.ts' '*.js' '*.cjs'",
    "typecheck": "tsc --noEmit",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:rust:check": "cd src-tauri && cargo fmt --check",
    "clean": "rm -rf dist coverage coverage-rust src-tauri/target",
    "prepare": "husky",
    "release": "release-it",
    "release:dry": "release-it --dry-run"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.2",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-notification": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-positioner": "^2.3.1",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash-es": "^4.17.22",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.562.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.5.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@eslint/js": "^9.39.2",
    "@release-it/conventional-changelog": "10.0.1",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.5",
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@testing-library/react": "^16.3.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "knip": "^5.78.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "release-it": "^19.2.3",
    "secretlint": "^11.2.5",
    "syncpack": "^13.0.4",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
}
